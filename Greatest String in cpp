#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;
	cin>>t;
	string s;
	long int q;
	for(int i=0;i<t;i++){
		cin>>s;
		cin>>q;
	
	for(int i=0;i<s.length() && q;i++){
		if(s[i]=='a' || s[i] =='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'){
			
			s[i]=s[i]+1;
			q--;
			
		}
		
	}for(int i=0;i<s.length();i++){
		cout<<s[i];
	}
	cout<<endl;
	
	}
	
}
